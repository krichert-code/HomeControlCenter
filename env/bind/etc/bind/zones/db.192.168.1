;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     s.dom. server.om. (
                              3         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
; name servers
      IN      NS      s.dom.
      IN      NS      server.dom.
      IN      NS      dom.dom.
     

; PTR Records
3   IN      PTR     s.dom.        ; 192.168.1.3

29  IN      PTR     gabinet.dom.  ; 192.168.1.29










